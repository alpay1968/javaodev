package ödev1;

public class MiniProjeAsalSayı {
public static void main(String[] args) {
	int number =25;
	boolean isPrime=true;
	int remainder= number%2;
	
	
	
	
for (int i = 2; i < number; i++) {
	if(number%i ==0) {
		isPrime=false;
	}
}
		if(isPrime==true) {
			System.out.println("Sayı asaldır");
		
		}
		else {
			System.out.println("Sayı asal değildir");
		}






	}
}

